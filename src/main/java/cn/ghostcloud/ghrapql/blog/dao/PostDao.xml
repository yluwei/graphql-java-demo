<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.ghostcloud.ghrapql.blog.dao.PostDao">

    <insert id="insert" parameterType="cn.ghostcloud.ghrapql.blog.entity.Post" keyProperty="id" useGeneratedKeys="true">
        insert into post (title,text,category,authorId)
        values
        (#{title},#{text},#{category},#{authorId})
    </insert>
</mapper>